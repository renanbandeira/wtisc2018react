{
  "name": "wtisc2018react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "gm-mobile-libs": "git+ssh://git@github.com/greenmilellc-org/gm-mobile-libs.git",
    "gm-react-components": "git+ssh://git@github.com/greenmilellc-org/gm-react-components.git",
    "immutable": "^3.8.2",
    "onsenui": "^2.9.2",
    "postcss-apply": "^0.10.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-nesting": "^5.0.0",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.2",
    "postcss-variables": "^1.1.1",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-leaflet-div-icon": "^1.1.0",
    "react-leaflet-draggable-polyline": "^1.0.3",
    "react-scripts": "1.1.4"
  },
  "scripts": {
    "prestart": "yarn prep:deps",
    "start": "react-scripts start",
    "copy:media": "rm -rf public/static && cp -r node_modules/gm-react-components/es/static public/static",
    "build": "react-scripts build",
    "build:css": "./node_modules/.bin/postcss src/theme/index.pcss -o src/theme/index.css -c postcss.config.js",
    "build:theme": "cd node_modules/gm-react-components/ && yarn build:css && cd -",
    "prep:deps": "yarn build:theme && npm-run-all -p --print-name build:css copy:media",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --ext .js --ext .jsx src test",
    "lint:fix": "eslint --ext .js --ext .jsx --fix src test",
    "upgrade:grc": "yarn upgrade git+ssh://git@github.com/greenmilellc-org/gm-react-components.git"
  },
  "devDependencies": {
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-react-app": "^2.1.0",
    "eslint-plugin-flowtype": "^2.46.3",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-react-perf": "^2.0.8",
    "eslint-plugin-standard": "^3.1.0",
    "npm-run-all": "^4.1.3",
    "postcss-apply": "^0.8.0",
    "postcss-cli": "^4.1.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-easy-import": "^3.0.0",
    "postcss-loader": "^2.1.0",
    "postcss-nesting": "^4.0.1",
    "postcss-reporter": "^5.0.0",
    "postcss-url": "^7.3.0",
    "postcss-variables": "^1.1.1"
  }
}
